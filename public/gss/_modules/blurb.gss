@if [Wwin] <= [breakpoint2] {
  blurb {
    min-width: == 4rem;
    font-weight: == 400;
    font-size: == 6;
  }
}
@else {
  @if [Wwin] >= [breakpoint1] {
    blurb {
      min-width: == 10rem;
      font-size: == 36;
    }
  }
  @else {
    blurb {
      min-width: == 6rem;
      font-size: == 16;
      font-weight: == 700;
    }
  }
}
